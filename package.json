{
  "name": "lynxjs-module",
  "version": "0.1.0",
  "description": "CLI and codegen for LynxJS native modules (scaffold + codegen)",
  "type": "module",
  "license": "MIT",
  "bin": {
    "lynxjs-module": "./dist/cli.js",
    "lynxjs-module/codegen": "./dist/codegen.js",
    "lynxjs-module/generate-install": "./dist/cli/generate-install.js"
  },
  "files": [
    "dist",
    "dist/cli",
    "templates"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli.js",
    "./codegen": "./dist/codegen.js",
    "./install": "./dist/install.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "prepare": "npm run build",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "bumpp": "^10.3.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.0",
    "ts-morph": "^27.0.2",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@lynx-js/types": "^3.4.11",
    "fast-glob": "^3.2.12",
    "filepermission": "link:filepermission",
    "fs-extra": "^11.3.2"
  },
  "module": "./dist/index.js",
  "packageManager": "bun@1.3.0"
}